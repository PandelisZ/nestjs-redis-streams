{
  "name": "@pandelis/nestjs-redis-streams",
  "version": "2.1.2",
  "description": "Redis Streams Transport for NestJS. A drop in replacement for the built-in microservice Redis transport.",
  "author": "Pandelis Zembashis <p@ndel.is>",
  "private": false,
  "license": "MIT",
  "main": "dist/index.js",
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "prebuild": "rm -rf dist",
    "build": "tsc -p tsconfig.json",
    "format": "prettier **/**/*.ts --ignore-path ./.prettierignore --write",
    "test": "jest",
    "test:cov": "jest --coverage",
    "deploy": "pnpm run build && npm publish --access public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/pandelisz/nestjs-redis-streams.git"
  },
  "keywords": [
    "nest",
    "nestjs",
    "redis",
    "ioredis",
    "redis streams",
    "redis streams strategy",
    "redis streams transport",
    "nest redis streams strategy",
    "nest redis streams transport",
    "nest microservice redis"
  ],
  "bugs": {
    "url": "https://github.com/pandelisz/nestjs-redis-streams/issues"
  },
  "homepage": "https://github.com/pandelisz/nestjs-redis-streams#readme",
  "dependencies": {
    "@nestjs/common": "^11.1.6",
    "@nestjs/microservices": "^11.1.6",
    "ioredis": "^5.8.1",
    "rxjs": "^7.8.2",
    "tslib": "^2.8.1",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@nestjs/testing": "^11.1.6",
    "@types/express": "^5.0.3",
    "@types/jest": "30.0.0",
    "@types/node": "^24.7.0",
    "@types/supertest": "^6.0.3",
    "@types/uuid": "^11.0.0",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^30.2.0",
    "prettier": "^2.8.8",
    "sinon": "^21.0.0",
    "ts-jest": "29.4.4",
    "ts-loader": "^9.5.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  }
}
